{
  "rewrites": [
    {
      "source": "/",
      "destination": "/html/home.html"
    },
    {
      "source": "/login",
      "destination": "/html/login.html"
    },
    {
      "source": "/api/:path*",
      "destination": "/api/index"
    }
  ],
  "functions": {
    "api/index.ts": {
      "runtime": "edge"
    }
  }
}
