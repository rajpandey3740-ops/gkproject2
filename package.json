{
  "name": "gk-general-store",
  "version": "1.0.0",
  "description": "A modern e-commerce web application for a neighborhood general store",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
